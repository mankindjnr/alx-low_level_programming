#include "main.h"
#include <stdlib.h>
#include <stdlib.h>
/**
 *_strdup - duplicates the string
 *@str: the string to copy
 *Return: pointer to the duplicate string
 */
char *_strdup(char *str)
{
	int n;
	char *result;

	n = strlen(str);

	result = (char *)malloc(n * sizeof(char));

	if (str == NULL || result == NULL)
	{
		return (NULL);
	}
	else
	{
		result = strdup(str);
	}
	return (result);
}
